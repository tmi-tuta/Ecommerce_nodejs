<div class="container mt-2">
    <div class="flex items-center justify-between">
        <a class="text-white bg-blue-700 hover:bg-blue-800 focus:outline-none focus:ring-4 focus:ring-blue-300 
        font-medium rounded-md text-sm px-4 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 
        dark:focus:ring-blue-800 h-10 flex items-center justify-center" href="/admin/warehouse">Back</a>
        <button type="button" class="btn text-white bg-blue-700 hover:bg-blue-800 focus:outline-none focus:ring-4 focus:ring-blue-300 
        font-medium rounded-md text-sm px-4 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 
        dark:focus:ring-blue-800 h-10 flex items-center justify-center" data-bs-toggle="modal" data-bs-target="#import_warehouse">
            Import
        </button>
    </div>
    <div class="container-table mx-2">
        <table class="table table-striped table-hover data-table" style="max-width: 1300px;">
            <thead style="background-color: #07497c; color: white;">
                <tr>
                    <th class="text-center" colspan="5">Warehouse in</th>
                </tr>
                <tr>        
                    <th class="text-center">Quantity</th>
                    <th class="text-center">Unit price</th>
                    <th class="text-center">Total price</th>
                    <th class="text-center">User</th>
                    <th class="text-center">Date</th>
                </tr>
            </thead>
            <tbody>
                <% wareHouseins.forEach(wareHousein => { %>
                    <tr>
                        <td class="text-center"><%= wareHousein.quantity %></td>
                        <td class="text-center"><%= formatCurrency(wareHousein.price) %></td>
                        <td class="text-center"><%= formatCurrency(wareHousein.price*wareHousein.quantity) %></td>
                        <td class="text-center"><%= wareHousein.user_id.name %></td>
                        <td class="text-center"><%= formatDateTime(wareHousein.createdAt) %></td>
                    </tr>
                    <%- include('../../admin/modals/import_warehouse.ejs', { wareHousein: wareHousein }) %>
                <% }) %>
            </tbody>
        </table>
    </div>
</div>